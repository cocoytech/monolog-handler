{
    "name": "app-insights-php/monolog-handler",
    "description": "Microsoft App Insights monolog handler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Abayon",
            "email": "franz.noyaba@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        }
    },
        "repositories":
    [
            {
                "type": "vcs",

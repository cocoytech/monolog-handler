    "require": {
        "monolog/monolog": "^1.7",
        "app-insights-php/client": "dev-master as 0.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1 || ^8.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
